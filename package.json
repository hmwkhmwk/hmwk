{
  "name": "monday-integration-quickstart-app",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "description": "Collect homework assignments and keep track of them in one place",
  "main": "src/app.js",
  "private": true,
  "engines": {
    "node": "14.15.0"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "expose": "ngrok http 8302",
    "get-public-address": "addr=$(curl http://localhost:4040/api/tunnels | jq -r '.tunnels | .[] | select( .name == \"command_line\" ) | .public_url') && echo \"\nYour public URL is $addr\"",
    "reseed": "node src/scripts/reseed.js",
    "start": "npm run stop && npm run server",
    "server": "node ./src/app.js",
    "server-dev": "webpack --config webpack.config.js --watch & NODE_ENV=dev nodemon ./src/app.js",
    "server-with-debugger": "nodemon --delay 100ms --exec 'fuser -k 9228/tcp; node --inspect=0.0.0.0:9228 ./src/app.js'",
    "stop": "kill-port 8302 && kill-port 4049 && kill-port 4040 && kill-port 5000",
    "test": "jest"
  },
  "nodemonConfig": {
    "ignore": [
      "hmwk-db.json"
    ]
  },
  "dependencies": {
    "async-lock": "^1.2.4",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "css-loader": "^5.0.1",
    "dotenv": "^5.0.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "history": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "monday-sdk-js": "0.0.12",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "node-json-db": "^1.1.0",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "proxyquire": "^2.0.1",
    "querystring": "^0.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^3.11.0",
    "react-measure": "^2.5.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sequelize": "^5.21.5",
    "socket.io": "^3.0.1",
    "style-loader": "^2.0.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "babel-loader": "^8.2.1",
    "chai": "^4.1.2",
    "concurrently": "^5.2.0",
    "cross-port-killer": "^1.2.1",
    "debug": "~2.6.9",
    "eslint": "^v6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jq-cli-wrapper": "^1.6.1",
    "lint-staged": "^10.5.1",
    "mocha": "^5.0.4",
    "mock-require": "^3.0.1",
    "ngrok": "^3.2.7",
    "nodemon": "^2.0.2",
    "nyc": "^12.0.2",
    "prettier": "^2.1.2",
    "sequelize-cli": "^5.5.1",
    "sinon": "^7.4.2",
    "sqlite3": "^4.1.1",
    "webpack": "^5.5.0",
    "webpack-cli": "^4.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
